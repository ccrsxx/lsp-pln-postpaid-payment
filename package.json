{
  "name": "learnify-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "sort-imports": "next lint --fix .",
    "type-check": "tsc --noEmit",
    "format": "prettier --check .",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky install",
    "db:drop": "dotenv -e .env.local -- cross-env-shell \"echo DROP DATABASE IF EXISTS $DATABASE_TO_DROP\" | dotenv -e .env.local -- prisma db execute --stdin",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:create": "dotenv -e .env.local -- cross-env-shell \"echo CREATE DATABASE $DATABASE_TO_CREATE\" | dotenv -e .env.local -- prisma db execute --stdin",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:create-only": "dotenv -e .env.local -- prisma migrate dev --create-only"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-query": "^5.8.7",
    "@tanstack/react-table": "^8.19.3",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv-cli": "^7.4.2",
    "framer-motion": "^10.16.12",
    "lucide-react": "^0.414.0",
    "next": "^14.0.3",
    "next-auth": "^5.0.0-beta.19",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-otp-input": "^3.1.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "^18",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "lint-staged": "^14.0.0",
    "postcss": "^8",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.17.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3",
    "tsx": "^4.16.2",
    "typescript": "^5"
  },
  "prisma": {
    "schema": "./src/lib/prisma/schema.prisma",
    "seed": "tsx ./src/lib/prisma/seed.ts"
  }
}
